{
  "name": "stock2shop/logger",
  "description": "Stock2Shop Logging",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Stock2Shop\\Logger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Stock2Shop\\Tests\\Logger\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/stock2shop/share"
    },
    {
      "type": "vcs",
      "url": "https://github.com/stock2shop/environment"
    }
  ],
  "authors": [
    {
      "name": "Stock2Shop PTY (Ltd)",
      "email": "support@stock2shop.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "monolog/monolog": "^2.8.0",
    "stock2shop/share": "^0.2",
    "stock2shop/environment": "^0.2",
    "aws/aws-sdk-php": "^3.238",
    "maxbanton/cwh": "^2.0.2",
    "vlucas/phpdotenv": "^5.4",
    "ext-json": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.12",
    "phpunit/phpunit": "^9",
    "vimeo/psalm": "^4.29"
  }
}
